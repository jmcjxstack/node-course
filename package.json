{
	"name": "node-course",
	"version": "1.0.0",
	"description": "node course",
	"main": "./src/index.ts",
	"type": "commonjs",
	"scripts": {
		"build": "tsc",
		"start": "node dist/index.js",
		"dev": "nodemon --exec ts-node src/index.ts",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"typeorm": "npx typeorm-ts-node-commonjs --dataSource src/data-source.ts",
		"typeorm:migrate": "npm run typeorm migration:run",
		"typeorm:generate": "npm run typeorm migration:generate src/migration/Migration --timestamp",
		"typeorm:revert": "npm run typeorm migration:revert"
	},
	"author": "Jose Miguel Carvajal Jimenez",
	"license": "ISC",
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.10",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/node": "^20.11.7",
		"@types/swagger-ui-express": "^4.1.6",
		"@types/uuid": "^9.0.7",
		"@types/yamljs": "^0.2.34",
		"jest": "^29.7.0",
		"nodemon": "^3.0.1",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"axios": "^1.6.2",
		"cors": "^2.8.5",
		"csvtojson": "^2.0.10",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"joi": "^17.11.0",
		"jsonwebtoken": "^9.0.2",
		"pg": "^8.11.3",
		"reflect-metadata": "^0.2.1",
		"swagger-ui-express": "^5.0.0",
		"typeorm": "^0.3.20",
		"uuid": "^9.0.1",
		"yamljs": "^0.3.0"
	}
}